# Airtable Configuration
# Get these from your Airtable account settings

# Your Airtable API Key (starts with pat_...)
VITE_AIRTABLE_API_KEY=your_airtable_api_key_here

# Your Airtable Base ID (starts with app...)
VITE_AIRTABLE_BASE_ID=app0tFfsjLbI1qXq0

# Optional: Override table name (default is "Directory Submissions")
# VITE_AIRTABLE_TABLE_NAME=Directory Submissions

# Stripe Configuration
# Get these from your Stripe Dashboard

# Your Stripe Publishable Key (starts with pk_test_... for test mode)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Client URL for redirects
VITE_CLIENT_URL=http://localhost:5173 